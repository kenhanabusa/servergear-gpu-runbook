#!/usr/bin/env bash
set -euo pipefail
IFS=$'\n\t'
STK_ID="STK-016"
DIR="$(cd "$(dirname "$0")" && pwd)"
# shellcheck disable=SC1091
source "$DIR/../_lib/sg_runbook_lib.sh"
latest="$(ls -1t /var/log/sg-runbook/${STK_ID}_*.jsonl 2>/dev/null | head -n 1 || true)"
[[ -n "$latest" ]] || latest="$(ls -1t "$HOME/sg-runbook-log/${STK_ID}_*.jsonl" 2>/dev/null | head -n 1 || true)"
echo "[doctor] rid=${STK_ID}"
echo "[doctor] latest_jsonl=${latest:-<none>}"
